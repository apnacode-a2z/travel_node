<div class="hero-container" >
        <div class="hero-cell w-55" style="padding-top:0px">
          <div class="container-fluid extra" >
            <div class="row">
              <div class="col-md-12">
                  <div class="form-conatiner">
                    <h1> Request Quotes</h1>
                      <form class="form" action="/admin/admin_lead_creation" method="POST" onsubmit="return submits()">
                        <div class="col-md-6">
                          <div class="form-group">
                            <label for="">From</label>

                            
                            <input type="text" name="from" class="form-control" required>
                            <i class="fa fa-fighter-jet" aria-hidden="true"></i>
                          </div>
                        </div>
                        <div class="col-md-6">
                          <div class="form-group">
                              <label for="">To</label>
                              <input type="text"  name="to"class="form-control" required>
                              <i class="fa fa-map-marker" aria-hidden="true"></i>
                          </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-group">
                              <label for="">Departure</label>
                              <input type="date" name="depart" class="form-control" id="departure" required>
                              <i class="fa fa-calendar-o" aria-hidden="true"></i>
                            </div>
                          </div>
                          <div class="col-md-6">
                            <div class="form-group">
                                <label for="">Return</label>
                                <input type="date"  name="dreturn" class="form-control" id="dreturn" required>
                                <i class="fa fa-calendar-o" aria-hidden="true"></i>
                            </div>
                          </div>
                          <div class="col-md-6">
                                <div class="form-group">
                                    <label for="">Passenger</label>
                                  <input type="number"  name="passengers" class="form-control" required>
                                  <i class="fa fa-user" aria-hidden="true"></i>
                                </div>
                              </div>

                          <div class="col-md-6">
                              <div class="form-group">
                                  <label for="">Full Name</label>
                                <input type="text"  name="fullname" class="form-control" required>
                                <i class="fa fa-user" aria-hidden="true"></i>
                              </div>
                            </div>
                            
                            <div class="col-md-12">
                                <div class="form-group">
                                      <label for="">Email id</label>
                                  <input type="email" name="email" class="form-control" required>
                                  <i class="fa fa-envelope" aria-hidden="true"></i>
                                </div>
                              </div>
                            <div class="col-md-12">
                              <div class="form-group">
                                  <input type="submit" value="Get Quote" class="form-control">
                              </div>
                            </div>
                            <!-- <button type="button" onClick="submits()">Click me</button> -->
                      </form>
                    </div>
              </div>
            </div>
          </div>
        </div>
</div>

<script src="../javascripts/validate_date.js"></script> 
