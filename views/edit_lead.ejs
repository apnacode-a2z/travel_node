<form class="form-horizontal edit-lead-box" action="/admin/lead_updation/<%=data.Lead[0].id%>" 
    method="POST">
    <% for(var i=0; i < data.Lead.length; i++) { %> 
    <div class="form-group">
      <label class="control-label col-sm-2" for="form"> From:</label>
      <div class="col-sm-6">
        <input type="text" class="form-control" name="from" id="from" value="<%= data.Lead[i].from %>" >
      </div>
    </div>
    <div class="form-group">
      <label class="control-label col-sm-2" for="to"> To:</label>
      <div class="col-sm-6"> 
        <input type="text" class="form-control" name="to" id="to" value="<%= data.Lead[i].to %>">
      </div>
    </div>
    <div class="form-group">
        <label class="control-label col-sm-2" for="depart"> Depart:</label>
        <div class="col-sm-6">
            <input type="date" class="form-control" name="depart" id="depart" value="<%= date_format(data.Lead[i].depart, 'yyyy-mm-dd') %>">
        </div>
    </div>
    <div class="form-group">
        <label class="control-label col-sm-2" for="dreturn"> Return:</label>
        <div class="col-sm-6">
            <input type="Date" class="form-control" name="dreturn" id="dreturn" value="<%= date_format(data.Lead[i].dreturn, 'yyyy-mm-dd') %>">
        </div>
     </div>
     <div class="form-group">
            <label class="control-label col-sm-2" for="passengers"> Passengers:</label>
            <div class="col-sm-6">
              <input type="number" class="form-control" name="passengers"  id="passengers" value="<%= data.Lead[i].passengers %>">
            </div>
    </div>
    <div class="form-group">
        <label class="control-label col-sm-2" for="fullname"> Full Name:</label>
        <div class="col-sm-6">
            <input type="text" class="form-control" name="fullname" id="fullname" value="<%= data.Lead[i].fullname %>" >
        </div>
    </div>
    <div class="form-group">
            <label class="control-label col-sm-2" for="email"> Email:</label>
            <div class="col-sm-6">
              <input type="email" class="form-control" name="email" id="email" value="<%= data.Lead[i].email %>">
            </div>
    </div>
    <div class="form-group">
        <div class="col-md-3">
            <select class=" btn btn-primary" id="foodItem0">
                <% for(var j=0; j < data.PreSalesAgent.length; j++) { %> 
                <option value="<%= data.PreSalesAgent[j].id%>"> <%= data.PreSalesAgent[j].Name %></option>
                <% } %>
            </select>

            <input name="PreSalesAgent_id" id="agent_id" hidden="true" />
        </div>
            <div class="col-md-3 ">
                    <input  class=" btn btn-primary" id="submit" type="submit" value="save" class="form-control"  >
            </div>
            <div class="col-md-3">
                    <input class=" btn btn-primary" type="submit" value="cancel" class="form-control" >
            </div>
            
    </div>
    <% } %>
    <script>
        $('#foodItem0').change(function() {
            var agent_type = $(this).val();
            $("#agent_id").val(agent_type);      
        });
    </script>
</form>


