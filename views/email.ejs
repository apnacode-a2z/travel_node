<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap-theme.min.css" integrity="sha384-rHyoN1iRsVXV4nD0JutlnGaslCJuC7uwjduW9SVrLvRYooPp2bWYgmgJQIXwl/Sp" crossorigin="anonymous">
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js" integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa" crossorigin="anonymous"></script>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
    <style type="text/css">
        h1{
            color: royalblue;
        }
        h3{
            color: royalblue;
        }
              table, td, th {    
              border: 1px solid #ddd;
              text-align: left;
          }
          
          table {
              border-collapse: collapse;
              width: 100%;
          }
          
          th, td {
              padding: 15px;
          }
          td{
            font-size: 13px;
          }
          th {
            background-color: #eeeeee;
            font-size: 15px;
            font-weight: bold;
          }
         
           
          .edit-lead-box{
            padding: 40px;
            border: 2px solid #000000;
            width: 57%;
            position: absolute;
            left: 28%;
            
          }
          .view-customer-box{
            border: 2px solid #000000;
            max-width: 900px;
            margin: 60px 20px;
            padding: 20px;
          }
          .view-space{
            margin-top: 20px;
          }
          .view-table{
            margin: 10px 0 10px 0px;
            border: 1px solid gray;
            padding: 20px;
          }
          
        </style>
</head>
 
  <body>
       
        <div class="view-customer-box">
                <img src="cid:logo" alt="" style="width:290px;">
                <hr />
                
                <!-- <div class="container"> -->
                <% for(var i=0; i < todos.length; i++) { %>
            <div class="row">
                <div class="col-md-4 view-space">
                    <h3 ><strong style="width:400px"> Booking Refrence :</strong> <span >  MKS1C1</span></h3>
                </div>
                
                <div class="col-md-4 view-space">
                    <h3><strong style="width:400px"> Trip information :</strong> <span >  <%= todos[i].depart %> | <%= todos[i].from %> to <%= todos[i].to %></span></h3>
                </div>
                
                <div class="col-md-4 view-space">
                    <h3><strong style="width:400px"> Trip Duration :</strong> <span >  10h 35m</span></h3>
                </div>

                <div class="col-md-4 view-space">
                    <h3><strong style="width:400px"> E-Ticket #:</strong> <span >  <%= todos[i].fullname %></span></h3>
                </div>
            </div>
        <!-- </div> -->
        <div class="view-table">
            <div class="row ">
                <div class="col-md-5 view-space">
                        <img src="cid:logo1" alt="" style="width:165px;">
                </div>
                <div class="col-md-7 view-space">
                        <h3> OUTBOUNT FLIGHT </h3>
                </div>
                <div class="col-md-12 view-space">
                        <h3> <%= todos[i].from %> to <%= todos[i].to %> </h3>
                        <h3> <%= todos[i].depart %> </h3>
                </div>
            </div>
            <table class="view-space">
                <tr>
                        <th> DEPARTURE</th>
                        <th> FLIGHT INFORMATION</th>
                </tr>
                <tr>
                    <td> <%= todos[i].from %> <br>
                         <%= todos[i].depart %>
                     </td>
                    <td> <strong> Flight #: </strong> IN 901 <br />
                        <strong> Duration: </strong> 10hr 35min <br />
                        <strong> Cabin: </strong> Business Class <br />
                        <strong> Aircraft: </strong> AIRINDIA 777-303 <br />
                    </td>
                </tr>
          
                    <tr>
                            <th> ARRIVAL</th>
                            <th> SEAT INFORMATION</th>
                    </tr>
                    <tr>
                        <td> <%= todos[i].to %> <br>
                             <%= todos[i].dreturn %>
                         </td>
                        <td> 
                                <h3><%= todos[i].fullname %></h3>
                        </td>
                    </tr>
                </table>
        </div>
        <% } %>
</div>
</body>
</html>